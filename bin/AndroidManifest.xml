<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whattoeat"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.whattoeat.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.whattoeat.RestauruntActivity"
            android:label="@string/title_activity_restaurunt" >
        </activity>
        <activity
            android:name="com.example.whattoeat.ItemActivity"
            android:label="@string/title_activity_item" >
        </activity>
        <activity
            android:name="com.example.whattoeat.PagerActivity"
            android:label="@string/title_activity_pager" >
        </activity>
        <activity
            android:name="com.example.whattoeat.WebViewActivity"
            android:label="@string/title_activity_web_view" >
        </activity>
    </application>

</manifest>